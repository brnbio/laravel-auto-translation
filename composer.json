{
    "name": "brnbio/laravel-auto-translation",
    "description": "Auto translation package for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "BrnBio\\LaravelAutoTranslation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrnBio\\LaravelAutoTranslation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BrnBio\\LaravelAutoTranslation\\LaravelAutoTranslationServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    }
}
